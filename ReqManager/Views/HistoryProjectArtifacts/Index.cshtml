@model IEnumerable<ReqManager.Entities.Artifact.HistoryProjectArtifactEntity>

@{
    ViewBag.Title = "Artifact History Listing";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProjectArtifact.code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descriptionTypeArtifact)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.descriptionMeasureImportance)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.path)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.creationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.login)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectArtifact.code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descriptionTypeArtifact)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.descriptionMeasureImportance)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.path)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.creationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.login)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.HistoryArtefactID }) 
        </td>
    </tr>
}

</table>
